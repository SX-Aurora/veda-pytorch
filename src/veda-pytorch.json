{
	"name":				"veda-pytorch",
	"version":			"@VEDAPytorch_VERSION@",
	"platform":			"linux_x86_x64",
	"summary":			"VEDA PyTorch",
	"readme":			"veda/pytorch/README.md",
	"author":			"__GIT__",
	"author-email":		"__GIT__",
	"license":			"3 BSD-License",
	"license-file":		"veda/pytorch/LICENSE",
	"requires-python":	">=3.7",
	"download-url":		"https://github.com/sx-aurora/veda-pytorch/tags",
	"classifier": [
		"License :: OSI Approved :: BSD License",
		"Programming Language :: Python :: 3 :: Only",
		"Programming Language :: C",
		"Programming Language :: C++"
	],
	"packages": [
		"veda/pytorch"
	],
	"payload": [
		"veda/pytorch/csrc/veda/pytorch/*",
		"veda/pytorch/csrc/veda/pytorch/c10d/*"
	],
	"requires-dist": [
		"veda (>= 2.3.0, < 3)",
		"tungl (>= 0.1.5, < 1)",
		"veda-tensors (>= 0.1.9, < 2)",
		"torch",
		"ninja",
		"packaging"
	],
	"entry-points": {
		"torch.backends": "veda_pytorch = veda.pytorch:autoload"
	}
}
{
	"name":				"veda-pytorch",
	"version":			"@PYTORCH_VERSION@.post@VEDAPytorch_VERSION@",
	"platform":			"linux_x86_x64",
	"summary":			"VEDA PyTorch",
	"readme":			"veda/pytorch/README.md",
	"author":			"__GIT__",
	"author-email":		"__GIT__",
	"license":			"3 BSD-License",
	"license-file":		"veda/pytorch/LICENSE",
	"requires-python":	">=3.7",
	"download-url":		"https://github.com/sx-aurora/veda-pytorch/tags",
	"classifier": [
		"License :: OSI Approved :: BSD License",
		"Programming Language :: Python :: 3 :: Only",
		"Programming Language :: C",
		"Programming Language :: C++"
	],
	"packages": [
		"veda/pytorch"
	],
	"payload": [
		"veda/pytorch/lib64/libveda-pytorch.so"
	],
	"requires-dist": [
		"veda (>= @VEDA_VERSION@, < @VEDA_VERSION_NEXT_MAJOR@)",
		"tungl (>= @Tungl_VERSION@, < @Tungl_VERSION_NEXT_MINOR@)",
		"veda-tensors (>= @VEDATensors_VERSION@, < @VEDATensors_VERSION_NEXT_MINOR@)",
		"torch==@PYTORCH_VERSION@"
	],
	"entry-points": {
		"torch.backends": "veda_pytorch = veda.pytorch:autoload"
	}
}